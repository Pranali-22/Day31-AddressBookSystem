/*UC1*/
create database address_book_system;
use address_book_system;

/*UC2*/
create table address_book(
	firstname varchar(20),
    lastname varchar(20),
	address varchar(50),
    city varchar(20),
    state varchar(20),
    zip char(6),
    phonenumber varchar(10),
    emailId varchar(30),
    primary key(firstname)
);